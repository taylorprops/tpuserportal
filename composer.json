{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "authorizenet/authorizenet": "^2.0",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-dompdf": "^1.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "kyslik/column-sortable": "^6.4",
        "laravel/framework": "^9.2",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mikehaertl/php-pdftk": "^0.11",
        "mojtabaahn/laravel-web-logs": "^1.4",
        "monolog/monolog": "^2.3",
        "phpoffice/phpspreadsheet": "^1.22",
        "rennokki/laravel-eloquent-query-cache": "^3.3",
        "romanzipp/laravel-queue-monitor": "^2.3",
        "shvetsgroup/laravel-email-database-log": "^10.0",
        "spatie/laravel-backup": "^8.0",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0",
        "theiconic/name-parser": "^1.2",
        "thiagoalessio/tesseract_ocr": "^2.12",
        "troydavisson/phrets": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "laravel/breeze": "^1.8",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/patrol": "^1.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/AuthNet.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
