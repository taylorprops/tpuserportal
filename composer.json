{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "authorizenet/authorizenet": "^2.0",
        "awobaz/compoships": "^2.1",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.6",
        "kyslik/column-sortable": "^6.0",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "lorisleiva/artisan-ui": "^0.3.0",
        "maatwebsite/excel": "^3.1",
        "mikehaertl/php-pdftk": "^0.10.1",
        "mojtabaahn/laravel-web-logs": "^1.3",
        "monolog/monolog": "^2.3",
        "phpoffice/phpspreadsheet": "^1.20",
        "rennokki/laravel-eloquent-query-cache": "^3.0",
        "romanzipp/laravel-queue-monitor": "^2.1",
        "shvetsgroup/laravel-email-database-log": "^9.0",
        "spatie/laravel-backup": "7.0.0",
        "theiconic/name-parser": "^1.2",
        "thiagoalessio/tesseract_ocr": "^2.11",
        "troydavisson/phrets": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "imanghafoori/laravel-microscope": "^1.0",
        "laravel/breeze": "^1.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/patrol": "^1.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/AuthNet.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },

    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
